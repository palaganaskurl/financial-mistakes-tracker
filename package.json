{
  "name": "financial-mistakes-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy --env=production",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload --env=development",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
    "db:push": "npx drizzle-kit push",
    "db:generate": "npx drizzle-kit generate",
    "db:migrate": "npx drizzle-kit migrate",
    "db:pull": "npx drizzle-kit pull",
    "db:studio": "npx drizzle-kit studio",
    "wrangler-dev": "wrangler dev",
    "wrangler-types": "wrangler types --env-interface CloudflareEnv",
    "wrangler-migrations": "wrangler d1 migrations apply financial-mistakes-tracker-d1 --local",
    "wrangler-migrations-remote": "wrangler d1 migrations apply financial-mistakes-tracker-d1 --remote"
  },
  "dependencies": {
    "@chakra-ui/cli": "^3.32.0",
    "@chakra-ui/react": "^3.32.0",
    "@emotion/react": "^11.14.0",
    "@hookform/resolvers": "^5.1.0",
    "@opennextjs/cloudflare": "^1.6.5",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.4",
    "iron-session": "^8.0.4",
    "next": "15.3.8",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "uuid": "^13.0.0",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260203.0",
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "typescript": "^5",
    "wrangler": "^4.62.0"
  }
}
